{
  "name": "globals-vitest",
  "description": "Autogenerated Vitest globals that are always up-to-date! ‚è∞",
  "version": "4.0.8",
  "main": "index.mjs",
  "types": "index.d.mts",
  "type": "module",
  "scripts": {
    "build": "node ./scripts/build.mjs && tsc index.mjs --declaration --emitDeclarationOnly --allowJs --moduleResolution bundler --target esnext && pnpm fix",
    "release": "release-it",
    "eslint": "eslint .",
    "prettier": "prettier . --check",
    "lint": "pnpm eslint && pnpm prettier",
    "fix": "pnpm eslint --fix && prettier . --write"
  },
  "devDependencies": {
    "@frsource/eslint-config": "workspace:*",
    "@frsource/prettier-config": "workspace:*",
    "@frsource/release-it-config": "workspace:*",
    "eslint": "^9.1.0",
    "globals": "^16.0.0",
    "import-meta-resolve": "^4.2.0",
    "prettier": "^3.2.5",
    "release-it": "^19.0.0",
    "typescript": "^5.4.5",
    "vitest": "4.0.8"
  },
  "peerDependencies": {
    "vitest": "4.0.8"
  },
  "homepage": "https://github.com/FRSOURCE/toolkit/tree/main/packages/globals-vitest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FRSOURCE/toolkit.git",
    "directory": "packages/globals-vitest"
  },
  "bugs": {
    "url": "https://github.com/FRSOURCE/toolkit/issues"
  },
  "author": "Jakub Freisler <jakub@frsource.org>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.20.0",
  "keywords": [
    "eslint",
    "vitest",
    "globals",
    "eslint globals",
    "eslint env",
    "eslint config",
    "eslint extends",
    "vitest eslint"
  ],
  "funding": "https://buymeacoffee.com/frsource"
}
