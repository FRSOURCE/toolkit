{
  "name": "@frsource/prettier-config",
  "version": "1.2.0",
  "main": "index.mjs",
  "types": "index.d.mts",
  "type": "module",
  "scripts": {
    "build": "tsc index.mjs --declaration --emitDeclarationOnly --allowJs --moduleResolution bundler --target esnext",
    "release": "release-it",
    "eslint": "eslint .",
    "prettier": "prettier . --check",
    "lint": "pnpm eslint && pnpm prettier",
    "fix": "pnpm eslint --fix && prettier . --write"
  },
  "devDependencies": {
    "@frsource/release-it-config": "workspace:*",
    "@frsource/eslint-config": "workspace:*",
    "prettier": "^3.2.5",
    "release-it": "^17.2.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "homepage": "https://github.com/FRSOURCE/toolkit/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FRSOURCE/toolkit.git",
    "directory": "packages/prettier-config"
  },
  "bugs": {
    "url": "https://github.com/FRSOURCE/toolkit/issues"
  },
  "author": "Jakub Freisler <jakub@frsource.org>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.15.7",
  "keywords": [
    "prettier",
    "prettier config"
  ],
  "funding": "https://buymeacoffee.com/frsource"
}
