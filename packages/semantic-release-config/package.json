{
  "name": "@frsource/semantic-release-config",
  "version": "1.72.0",
  "main": "index.mjs",
  "types": "index.d.mts",
  "scripts": {
    "build": "tsc index.mjs --declaration --emitDeclarationOnly --allowJs --moduleResolution bundler --target esnext",
    "release": "release-it",
    "eslint": "eslint .",
    "prettier": "prettier . --check",
    "lint": "pnpm eslint && pnpm prettier",
    "fix": "pnpm eslint --fix && prettier . --write"
  },
  "dependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.3",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3"
  },
  "devDependencies": {
    "@frsource/eslint-config": "workspace:*",
    "@frsource/prettier-config": "workspace:*",
    "@frsource/release-it-config": "workspace:*",
    "@types/node": "22.15.32",
    "eslint": "^9.1.0",
    "globals": "16.2.0",
    "prettier": "^3.2.5",
    "release-it": "19.0.3",
    "semantic-release": "24.2.5",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "semantic-release": ">= 24"
  },
  "homepage": "https://github.com/FRSOURCE/toolkit/tree/main/packages/semantic-release-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FRSOURCE/toolkit.git",
    "directory": "packages/semantic-release-config"
  },
  "bugs": {
    "url": "https://github.com/FRSOURCE/toolkit/issues"
  },
  "author": "Jakub Freisler <jakub@frsource.org>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.11.0",
  "keywords": [
    "semantic-release",
    "semantic-release config"
  ],
  "funding": "https://buymeacoffee.com/frsource"
}
